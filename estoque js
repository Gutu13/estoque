var online = false;
var produtodescri = false;

  if(localstorage.getitem('acesso') = true{
      online = true;
      console.log('inserido');
}
     
   if(online!= true){
      alert('usuario n√£o cadastrado');
      window.location.href = 'login.html';
}

  function controledeproduto(){
       exibirproduto = !exibirproduto;
       let form = document.getElementByID('formProduto');
       
       if(exibirproduto){
            form.style.display = 'block';
            }
            else{
                form.style.display = 'nome';
                } 
      }
 
      
}
  
  
